using Xamarin.Forms;

namespace EmeciGallery.Views
{
    public partial class MasterGalleryView : MasterDetailPage
    {
        public MasterGalleryView()
        {
            InitializeComponent();
        }

		protected override void OnAppearing()
		{
			base.OnAppearing();
			App.Master = this;
			App.Navigator = Navigator;
		}
    }
}
